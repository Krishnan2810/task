{%- liquid
    if section.blocks.size > 0 
    assign listWidth = section.settings.content_width | split: '/'
    assign leftColumn = listWidth[0]
    assign rightColumn = listWidth[1]  
    
    assign listWidth = section.settings.lap_content_width | split: '/' 
    assign lap_leftColumn = listWidth[0]
    assign lap_rightColumn = listWidth[1] 
     endif 
-%}
<script src="{{ 'inview.js' | asset_url }}" defer></script>
{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.35 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 1200px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
  .whistle-home-custom .home-custom-section .home-custom-section-wrapper {
    position: relative;
    z-index: 1;
    display: grid;
    grid-template-columns: 750px 1fr;
}
  .whistle-home-custom .home-custom-section .home-custom-section-wrapper .dt-sc-image-gallery { display: grid; grid-template-columns: repeat(2, 1fr);
    gap: var(--DTGutter_Width); gap: {{section.settings.column_gap}}px;
      }
  .whistle-home-custom .home-custom-section-wrapper .dt-sc-custom-block h6.dt-sc-sub-heading {
    font-size: 1.8rem;
    text-transform: uppercase;
    font-weight: 600;
    
}
  
  {% for block in section.blocks %}
    .whistle-home-custom .home-custom-section-wrapper .dt-sc-custom-block h6.dt-sc-sub-heading {
    color: {{ block.settings.sub_heading_color }};  
    }
  .whistle-home-custom .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block.{{block.type}}-{{block.id}} .dt-sc-heading:empty { display: none; }
  {% if block.settings.fullwidth_images %}
  .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block.{{block.type}}-{{block.id}} .dt-sc-image-gallery { grid-template-columns: 1fr; }
  {% endif %}
  {% endfor %}
  .whistle-home-custom .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block { display: flex; flex-wrap: wrap; justify-content: space-between; width: 100%;padding: 0; margin-top: 2rem;}

  .whistle-home-custom .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block.dt-sc-reverse-columns { flex-direction: column-reverse; }
  .whistle-home-custom .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block.dt-sc-reverse-columns .dt-sc-heading {
    margin-top: 50px; margin-bottom: 0; }
  .whistle-home-custom .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading > *:not(:last-child){ margin:0;}
  .whistle-home-custom .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading{ margin-bottom:20px; }
  .whistle-home-custom .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading.text-center{ text-align:center; }
  .whistle-home-custom .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading.text-start{ text-align:left; }
  .whistle-home-custom .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading.text-end{ text-align:right; }
  .whistle-home-custom .home-custom-section{position:relative;overflow:hidden;}
  .whistle-home-custom .home-custom-section:after{
    content:'';
    background:url('{{ section.settings.mask_image | image_url: width: 1920 }}');
    width: 100%;
    height: 100%;
    max-height: 250px;
    position: absolute;
    background-size: cover;
    background-repeat: no-repeat;
    bottom: 0;
    z-index: -1;
    }
  
  .whistle-home-custom:after{
    content:'';
    background:url('{{ section.settings.left_bg_image_pattern | image_url: width: 1500 }}');
    width: 90%;
    height: 100%;
    position: absolute;
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center;
    left: 50%;
    transform: translateX(-50%);
    bottom: 0;
    z-index: -1
  }
  .whistle-home-custom .media {
    background: transparent;
    
}
  .left-image{position: relative;}
  .whistle-home-custom .left-image .media{
    max-width: 650px;
    max-height: 750px;
    height: 800px;
  }
  .left_content_image img {
    object-fit: contain;
}
  

{%- for block in section.blocks -%} 
.whistle-home-custom .progress-bar_1 .progress-{{ block.id }}-value{ 
  background-color: {{ block.settings.progress_bar_color }};
  height: 10px;
  position: relative;
  border-top-right-radius: 20px;
  border-bottom-right-radius: 20px;
  border-top-left-radius: 20px;
  border-bottom-left-radius: 20px; 
  border: 1px solid rgb(var(--color-border));
  width: {{ block.settings.range }}%;
}
   .shopify-section.reveal .whistle-home-custom .progress-bar_1 .progress-{{ block.id }}-value{
    animation: progress-bar--{{forloop.index}} 1500ms ease;
  }
    
  @keyframes progress-bar--{{forloop.index}}{
    0%{
      width: 0;
    }
    100%{
      width: {{ block.settings.range }}%;
    }
  }
 
  
.whistle-home-custom .progress-bar_1 .progress-{{ block.id }}-value .percentage {
  position: absolute;
  top: -64px;
  right: 0;
  left: auto;
  bottom: auto;
}
 {% endfor %} 
.whistle-home-custom .progress-bar_1 .range {
  position: relative;
/*   height: 15px; */
  width: 100%;
  background-color: rgb(227,215,198);
  margin-bottom: 30px;
  border-top-right-radius: 20px;
  border-bottom-right-radius: 20px;
  border-top-left-radius: 20px;
  border-bottom-left-radius: 20px;
}
.whistle-home-custom .progress-bar_1:last-child .range{ margin: 0; }
.whistle-home-custom .progress-bar_1 { width: 100%; }
.whistle-home-custom .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-main-heading{ max-width: 700px; margin-bottom: 2rem !important; }
.whistle-home-custom .home-custom-section .progress_wrapper .progress-bar-title{ margin-top: 0; font-size: 1.8rem; font-weight: 600;margin-bottom: 0; font-family: var(--font-additional-family); color: var(--gradient-base-accent-3); }
  .home-custom-section .home-custom-section-wrapper .progress_wrapper{
    margin-top: 1rem;
  }
{%- for block in section.blocks -%} 
  .whistle-home-custom .progress-bar-title-percent.progress-{{block.id}}{ 
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: {{ block.settings.range }}%;
    margin-bottom: 15px;
}
  @media(max-width:480px){
    .whistle-home-custom .progress-bar-title-percent.progress-{{block.id}} {width:100%}
  }
   {% endfor %} 
.whistle-home-custom p.percentage {
    margin: 0;
    font-size: 2.2rem;
    line-height: normal;
    font-family: var(--font-additional-family);
  color: var(--gradient-base-accent-3);
    font-weight: 600;
}
.whistle-home-custom .home-custom-section-wrapper .dt-sc-custom-block a.button {
    margin-top: 5.2rem;
  background: rgb(var(--color-base-outline-button-labels));
}
  .whistle-home-custom .home-custom-section-wrapper .dt-sc-custom-block a.button:hover{
 background: rgb(var(--color-base-solid-button-labels));   
  }
  .whistle-home-custom .svg_icon_content_section{
    position: absolute;
    bottom: -30px;
    left: 0px;
    z-index: -1;
  }
  .whistle-home-custom .home-custom-section .home-custom-section-wrapper{
    padding: 0 4rem;
  }
  @media screen and (min-width:1024px) and (max-width:1540px){
    .whistle-home-custom .home-custom-section .home-custom-section-wrapper{grid-template-columns: 550px 1fr;column-gap: 50px;}
  .svg_icon_content_section svg{max-width: 550px;padding: 0;}
  }

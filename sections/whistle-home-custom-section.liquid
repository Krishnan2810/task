  .whistle-home-custom .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-main-heading{ font-size: 3rem; }
  .whistle-home-custom .home-custom-section .progress_wrapper .progress-bar-title{ font-size: 2rem; }
